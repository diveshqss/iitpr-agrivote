{
  "name": "AgriVote",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/AgriVote",
  "projectType": "application",
  "tags": ["scope:frontend", "type:app"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/AgriVote && npx react-router dev --port 3000"
      }
    },
    "serve:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/AgriVote && npx react-router dev --port 3000"
      }
    },
    "serve:prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/AgriVote && npx react-router dev --mode production --port 3001"
      }
    },
    "build": {
      "executor": "@react-router/dev:build",
      "outputs": ["{projectRoot}/build"],
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "build:dev": {
      "executor": "@react-router/dev:build",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "mode": "development"
      }
    },
    "build:prod": {
      "executor": "@react-router/dev:build",
      "outputs": ["{projectRoot}/build-prod"],
      "options": {
        "mode": "production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit"
      }
    }
  }
}
